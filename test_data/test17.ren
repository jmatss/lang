external function printf(format: {u8}, ...) -> i32

function main() -> i32
{
    var k = 0
    defer printf("\nHELLO - k: %d", k)
    if true
    {
        k++
    }

    var i = 0
    var abc = true
    while i < 3
    {
        // Should be executed at the end of every iteration.
        defer printf("\ni: %d", i)
        i++
        if abc
        {
            continue
        }
        else
        {
            break
        }
    }
    
    var x = 0
    var early_return = false
    {
        defer printf("\na, x: %d", x)
        x++

        if early_return
        {
            return 0
        }
        else
        {
            x += 2
        }

        defer printf("\nb, x: %d", x)
        x++
    }

    return 0
}