use std.assert

struct TestStruct
{
    a: i32,
    b: f64,
}

fn main() -> i32
{
    var test_struct = TestStruct { 123, 456.789 }
    var x = test_struct.a
    test_struct.b = 3.4

    printf("x: %d, test_struct.b: %f", x, test_struct.b)
    Assert::equals_i32(x, 123, "abc123")
    Assert::equals_f64(test_struct.b, 3.4, 0.001)

    return 0
}