use std::mem::Allocator

struct TestStruct
{
    inner_a: i8,
    inner_b: u8,
}

fn main() -> i32
{
    var i_u64: u64 = 123
    var a = Allocator::init<u64>(i_u64)
    defer Allocator::deinit<u64>(a)


    var b = Allocator::init_size<f32>(4)
    b.* = 678.0
    Allocator::deinit<f32>(b)


    var c = Allocator::init<TestStruct>()
    var d = Allocator::init<u64>(456)
    Allocator::deinit<u64>(d)
    Allocator::deinit<TestStruct>(c)


    var e = Allocator::init<u64>(123 as u64)
    var f = Allocator::reinit<u64>(e, 128)
    Allocator::deinit<u64>(f)

    return 0
}