use std::types::StringView

fn main() -> i32
{
    var view = StringView::new("test output")
    var res
    
    res = std::print(view)
    std::assert(res.&.is_success())

    res = std::print(view, line_break=false)
    std::assert(res.&.is_success())

    res = std::print(view, line_break=false, flush=true)
    std::assert(res.&.is_success())

    return 0
}
