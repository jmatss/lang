ext fn printf(format: {u8}, ...) -> i32

struct NestedStruct {
    var z: f64
    var b: {u32}
}

struct TestStruct {
    var x: u32
    var y: {NestedStruct}
}

fn double(n: {u32}) {
    n.* *= 2
}

fn main() -> i32 {
    var i = 3
    var s = NestedStruct { 123.321, i.& }
    var k = TestStruct { 456, s.& }
    double(k.y.*.b)
    k.y.*.b.* += 2
    std::assert(i == 8)
    printf(c"Output: %d", i)

    return 0
}
