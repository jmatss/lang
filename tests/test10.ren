use std::String

ext fn malloc(size: u32) -> {u8}

struct TestStruct {
    var i: i32
    var msg: {u8}
}

fn main() -> i32 {
    var test_struct = TestStruct { 0 , malloc(6) }

    if test_struct.i == 0 and false {
        test_struct.msg = c"TRUE"
    } else {
        test_struct.msg = c"FALSE"
    }

    while test_struct.i < 3 {
        test_struct.i += 1
    }

    printf(c"i: %d", test_struct.i)
    std::assert(test_struct.i == 3)

    return 0
}