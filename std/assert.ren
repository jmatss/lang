mod std

ext fn printf(format: {u8}, ...) -> i32
ext fn exit(exit_code: i32)
ext fn strlen(str: {u8}) -> u32

fn assert(expr: bool, msg: {u8} = "")
{
    if not expr
    {
        printf("\nASSERT FAILED\n")
        if strlen(msg) > 0
        {
            printf("%s\n", msg)
        }
        exit(1)
    }
}
